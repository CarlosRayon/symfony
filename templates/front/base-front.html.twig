<!DOCTYPE html>
<html>

    <head>
        <meta charset="UTF-8">
        <title>
            {% block title %}Welcome!
            {% endblock %}
        </title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('front', null, 'front') }}
            {{ encore_entry_link_tags('common', null, 'common') }}
        {% endblock %}
    </head>

    <body>

        {% include "common/header.html.twig" %}

        <main class="py-1"> {% block body %}{% endblock %}
            </main>

            {% include "common/footer.html.twig" %}

            {% block javascripts %}
                {{ encore_entry_script_tags('front',null,'front') }}
            {% endblock %}

        </body>

    </html>
</body>
</html>
